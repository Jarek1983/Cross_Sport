 <%= simple_form_for @practice do |f| %>
    <%= f.input :title, label: 'Podaj imię/pseudonim:', as: :wysihtml5 %>
    <%= f.input :subtitle, collection: ['Sprzęt', 'Zawody', 'Ogłoszenie', 'Trening', 'Komunikat'] %>
    <%= f.input :boxinfo, label: 'Podaj osiągnięcia:', as: :wysihtml5 %> 
    <%= f.input :image, label: "Zdjęcie" %>
    <%= f.input :image_second, label: "Zdjęcie" %>
    <%= f.input :image_third, label: "Zdjęcie" %>
    <%= f.input :boxtitle, collection: ['Michał', 'Marta', 'Tomek', 'Jarek'] %>
    <%= f.submit "Zapisz", class: 'btn btn-success' %>
<% end %>
