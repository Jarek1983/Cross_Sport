<section class="container">
  <div class="row", style="padding-top: 30px;">
      <div class="col-md-12">

<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @practice.title %>
</p>

<p>
  <strong>Subtitle:</strong>
  <%= @practice.subtitle %>
</p>

</div>
    </div>

 <p>
  <strong>Photo:</strong>
   <% if @practice.picone.present? %>
  <%= image_tag @practice.picone.url(:medium) %>
  <% else %>
  No image
  <% end %>
</p>

<p>
  <strong>Source:</strong>
  <%= @practice.boxtitle %>
</p>

<p>
  <strong>Info:</strong>
  <%= @practice.boxinfo %>
</p>

<%= link_to 'Edit', edit_practice_path(@practice) %> |
<%= link_to 'Back', practices_path %>
</section>
