<section class="container">
  <div class="row", style="padding-top: 50px;">
      <div class="col-md-12">

<p id="notice"><%= notice %></p>

<p>
  <strong>Tytuł:</strong>
  <%= @practice.title %>
</p>

<p>
  <strong>Kategoria:</strong>
  <%= @practice.subtitle %>
</p>

</div>
    </div>

 <p>
   <% if @practice.image.present? %>
  <%= image_tag @practice.image.url %>
  <% else %>
  No image
  <% end %>
</p>

<p>
  <strong>Twórca:</strong>
  <%= @practice.boxtitle %>
</p>

<p>
  <strong>Teść:</strong>
  <%= @practice.boxinfo %>
</p>

<%= link_to 'Edytuj', edit_practice_path(@practice), class: 'btn btn-primary' %> 
<%= link_to 'Powrót', practices_path, class: 'btn btn-success' %>
</section>
